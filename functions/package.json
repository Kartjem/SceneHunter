    "node": "20"
  },
  "main": "lib/index.js",
  "dependencies": {
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^6.0.1",
    "next": "15.3.3"
  },
  "devDependencies": {
    "typescript": "^4.9.5",
    "@types/node": "^18.15.0"
  },
  "private": true
}